{
  "name": "ag_front_web_app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "local": "cross-env NODE_ENV=local vite --mode development",
    "dev": "cross-env NODE_ENV=development vite --mode development",
    "test": "cross-env NODE_ENV=testing vite --mode development",
    "prod": "cross-env NODE_ENV=production vite --mode production",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier .  --write"
  },
  "dependencies": {
    "algoliasearch": "^4.24.0",
    "axios": "^1.7.2",
    "eslint": "^8.57.0",
    "firebase": "^10.12.2",
    "instantsearch.css": "^8.3.0",
    "instantsearch.js": "^4.73.2",
    "pinia": "^2.1.7",
    "typescript-eslint": "^7.16.0",
    "vite-plugin-babel": "^1.2.0",
    "vue": "^3.4.29",
    "vue-instantsearch": "^4.19.0",
    "vue-router": "^4.4.0",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.24.7",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "eslint-plugin-vue": "^9.27.0",
    "postcss": "^8.4.39",
    "prettier": "^3.3.2",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.2.2",
    "vite": "^5.3.2",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.0.21"
  }
}
